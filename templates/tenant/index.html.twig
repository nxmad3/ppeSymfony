{% extends 'base.html.twig' %}
{% block body %}

    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <th scope="row">{{ loop.index }}</th>
                <td>{{ user.lastName }}</td>
                <td>{{ user.name }}}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}